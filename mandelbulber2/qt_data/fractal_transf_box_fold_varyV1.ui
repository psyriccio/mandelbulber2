&lt;span style=&quot;color:#d0d0d0&quot;&gt;{&lt;/span&gt;
  &lt;span style=&quot;color:#f09479; font-weight:bold&quot;&gt;double&lt;/span&gt; limit &lt;span style=&quot;color:#d0d0d0&quot;&gt;=&lt;/span&gt; fractal&lt;span style=&quot;color:#d0d0d0&quot;&gt;-&amp;gt;&lt;/span&gt;mandelbox&lt;span style=&quot;color:#d0d0d0&quot;&gt;.&lt;/span&gt;foldingLimit&lt;span style=&quot;color:#d0d0d0&quot;&gt;;&lt;/span&gt;
  &lt;span style=&quot;color:#d0d0a0&quot;&gt;// double value = 2.0 *fractal-&amp;gt;mandelbox.foldingLimit;&lt;/span&gt;
  &lt;span style=&quot;color:#f09479; font-weight:bold&quot;&gt;double&lt;/span&gt; tempVC &lt;span style=&quot;color:#d0d0d0&quot;&gt;=&lt;/span&gt; limit&lt;span style=&quot;color:#d0d0d0&quot;&gt;;&lt;/span&gt; &lt;span style=&quot;color:#d0d0a0&quot;&gt;// constant to be varied&lt;/span&gt;

